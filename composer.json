{
    "name": "joaoinacio/doq",
    "description": "Quickly setup and use multiple configurations for containerized services using docker compose.",
    "keywords": ["docker", "docker-compose", "configuration"],
    "homepage": "https://github.com/joaoinacio/doq",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Inacio",
            "email": "joao.inacio@cleverti.com"
        }
    ],
    "support": {
         "issues": "https://github.com/joaoinacio/doq/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "^2.8 || ^3.0",
        "symfony/process": "^2.8 || ^3.0",
        "symfony/filesystem": "^2.8 || ^3.0",
        "symfony/finder": "^2.8 || ^3.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpmd/phpmd": "^2.5",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": { "doq\\": "src/doq" }
    },
    "bin": ["bin/doq"],
    "scripts": {
        "test": "phpunit"
    }
}
